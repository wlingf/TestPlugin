<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cc.jianke.testplugin">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/App_style">
        <activity
            android:name=".wanandroid.activity.IndexActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="cc.jianke.pluginmodule.ProxyActivity"/>
        <activity android:name=".SecondViewBindingMvvmActivity"/>
        <activity android:name=".TestDataBindActivity"/>
        <activity android:name=".wanandroid.activity.WebActivity"/>
        <activity android:name=".wanandroid.activity.LoginActivity"/>
        <activity android:name=".wanandroid.activity.CoinActivity"/>
        <activity android:name=".wanandroid.activity.CollectActivity"/>
    </application>

</manifest>